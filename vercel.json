{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "http://job-recommender-backend.onrender.com/api/$1" }
  ],
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/static-build", "config": { "distDir": "frontend/dist" } }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
